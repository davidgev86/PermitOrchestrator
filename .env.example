# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/permit_orchestrator
PGDATABASE=permit_orchestrator
PGHOST=localhost
PGPASSWORD=your_password
PGPORT=5432
PGUSER=username

# Redis Configuration (for background jobs)
REDIS_URL=redis://localhost:6379

# Object Storage Configuration (for documents and screenshots)
S3_ENDPOINT=https://s3.amazonaws.com
S3_BUCKET=permit-orchestrator
S3_ACCESS_KEY=your_access_key
S3_SECRET_KEY=your_secret_key

# Authentication
AUTH_SECRET=your_random_secret_key_here

# Magic Link Email Configuration
MAGICLINK_FROM=noreply@permitsync.local
