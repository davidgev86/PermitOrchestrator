# Application Configuration
NODE_ENV=development
PORT=5000

# External API Keys (if needed for geocoding, etc.)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
MAPBOX_ACCESS_TOKEN=your_mapbox_token

